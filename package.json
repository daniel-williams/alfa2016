{
  "name": "alfa.2016",
  "version": "0.0.1",
  "description": "Anna Lancaster Fine Art 2016 website",
  "main": "server.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js test/**/*.js*",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --display-error-details",
    "start": "babel-node server",
    "old:start:http": "http-server ./dist -a localhost -p 8000",
    "old:start:webpack": "webpack-dev-server --devtool eval --progress --colors --hot",
    "old:start": "npm-run-all --parallel old:start:webpack old:start:http"
  },
  "author": "Daniel James Williams",
  "license": "ISC",
  "dependencies": {
    "immutable": "^3.7.5",
    "react": "^0.14.3",
    "react-addons-pure-render-mixin": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.1",
    "redux": "^3.0.4",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "history": "^1.16.0",
    "html-webpack-plugin": "^1.7.0",
    "isomorphic-fetch": "^2.2.0",
    "mocha": "^2.3.4",
    "re-base": "^1.3.1",
    "react-hot-loader": "^1.3.0",
    "redux-thunk": "^1.0.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/daniel-williams/anna-lancaster-fine-art.git"
  }
}
